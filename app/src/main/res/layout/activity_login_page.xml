<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2CAD8F"
    tools:context=".LoginPage">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="239dp"
        android:layout_height="144dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="421dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.521"
        app:srcCompat="@drawable/img" />

    <View
        android:id="@+id/ellipse_1"
        android:layout_width="801dp"
        android:layout_height="477dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/ellipse_1"
        android:rotation="75"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="492dp" />

    <TextView
        android:id="@+id/kullanici_adi"
        android:layout_width="99dp"
        android:layout_height="32dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="171dp"
        android:layout_marginEnd="252dp"
        android:layout_marginBottom="23dp"
        android:text="Kullanıcı Adı"
        app:layout_constraintBottom_toTopOf="@+id/ellipse_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/çizgi1"
        android:layout_width="258dp"
        android:layout_height="7dp"
        app:srcCompat="@drawable/img_2"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="485dp" />

    <TextView
        android:id="@+id/sifre"
        android:layout_width="99dp"
        android:layout_height="32dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="350dp"
        android:layout_marginEnd="252dp"
        android:layout_marginBottom="23dp"
        android:text="Şifre"
        app:layout_constraintBottom_toTopOf="@+id/ellipse_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.581"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/çizgi2"
        android:layout_width="258dp"
        android:layout_height="7dp"
        app:srcCompat="@drawable/img_2"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="555dp" />

    <CheckBox
        android:id="@+id/BeniHatirlaKutu"
        android:layout_width="17dp"
        android:layout_height="17dp"
        android:layout_marginTop="21dp"
        app:layout_constraintTop_toTopOf="@+id/çizgi2"
        tools:layout_editor_absoluteX="60dp" />

    <TextView
        android:id="@+id/BeniHatirlaText"
        android:layout_width="73dp"
        android:layout_height="18dp"
        android:layout_marginStart="9dp"
        android:text="Beni Hatırla"
        app:layout_constraintStart_toEndOf="@+id/BeniHatirlaKutu"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="575dp" />

    <ImageButton
        android:id="@+id/button_giris_Yap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="88dp"
        android:background="00FFFFFF"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/BeniHatirlaText"
        app:srcCompat="@drawable/login_screen_giris_yap_button" />

    <ImageButton
        android:id="@+id/sifregoster"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:background="00FFFFFF"
        android:clickable="true"
        app:layout_constraintBottom_toTopOf="@+id/çizgi2"
        app:srcCompat="@drawable/sifre_goster"
        tools:layout_editor_absoluteX="293dp" />


</androidx.constraintlayout.widget.ConstraintLayout>